{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://t.me https://web.telegram.org;"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://t.me"
        }
      ]
    }
  ]
}